{
  "name": "vue.js-advanced-starter-template",
  "version": "0.10.0",
  "description": "A starter for Vue.js-projects",
  "keywords": [
    "vue.js",
    "vue.js router",
    "axios",
    "webpack 2",
    "animate.css"
  ],
  "author": "Wille Ristim√§ki <villeristimaki@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:villeristi/vue.js-advanced-starter-template.git"
  },
  "config": {
    "port": 3000
  },
  "scripts": {
