    "vuex-class": "^0.2.0",
    "vuex-router-sync": "^4.3.2",
    "webpack": "^3.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^0.14.0",
    "webpack-notifier": "^1.5.0"
